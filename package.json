{
  "name": "rdf-dataset-fragmenter",
  "version": "1.0.0",
  "description": "Fragments an RDF dataset into multiple parts",
  "keywords": [
    "rdf",
    "rdfjs",
    "dataset",
    "fragmenter",
    "decentralization"
  ],
  "main": "index.js",
  "typings": "index",
  "repository": "git@github.com:rubensworks/rdf-dataset-fragmenter.js.git",
  "author": "Ruben Taelman <rubensworks@gmail.com>",
  "bugs": {
    "url": "https://github.com/rubensworks/rdf-dataset-fragmenter.js/issues"
  },
  "homepage": "https://github.com/rubensworks/rdf-dataset-fragmenter.js#readme",
  "license": "MIT",
  "lsd:module": "https://linkedsoftwaredependencies.org/bundles/npm/rdf-dataset-fragmenter",
  "lsd:components": "components/components.jsonld",
  "lsd:contexts": {
    "https://linkedsoftwaredependencies.org/bundles/npm/rdf-dataset-fragmenter/^1.0.0/components/context.jsonld": "components/context.jsonld"
  },
  "lsd:importPaths": {
    "https://linkedsoftwaredependencies.org/bundles/npm/rdf-dataset-fragmenter/^1.0.0/components/": "components/",
    "https://linkedsoftwaredependencies.org/bundles/npm/rdf-dataset-fragmenter/^1.0.0/config/": "config/"
  },
  "files": [
    "bin/**/*.d.ts",
    "bin/**/*.js",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "index.d.ts",
    "index.js",
    "components"
  ],
  "dependencies": {
    "@types/lru-cache": "^5.1.0",
    "@types/mkdirp": "^1.0.1",
    "@types/rdf-js": "*",
    "componentsjs": "^3.6.0",
    "lru-cache": "^6.0.0",
    "mkdirp": "^1.0.4",
    "rdf-parse": "^1.5.0",
    "rdf-serialize": "^1.0.0",
    "rdf-string": "^1.5.0",
    "rdf-terms": "^1.6.2"
  },
  "pre-commit": [
    "build",
    "lint",
    "test"
  ],
  "devDependencies": {
    "@rubensworks/eslint-config": "^1.0.1",
    "@types/jest": "^26.0.0",
    "@types/rdf-js": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.1.1",
    "arrayify-stream": "^1.0.0",
    "componentsjs-generator": "^1.4.0",
    "coveralls": "^3.0.0",
    "eslint": "^7.9.0",
    "eslint-config-es": "^3.23.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-tsdoc": "^0.2.7",
    "eslint-plugin-unused-imports": "^0.1.3",
    "jest": "^26.0.0",
    "jest-rdf": "^1.5.0",
    "manual-git-changelog": "^1.0.0",
    "pre-commit": "^1.2.2",
    "rdf-data-factory": "^1.0.4",
    "stream-to-string": "^1.2.0",
    "streamify-array": "^1.0.1",
    "streamify-string": "^1.0.1",
    "ts-jest": "^26.0.0",
    "typescript": "^4.0.2"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "scripts": {
    "test": "jest ${1}",
    "test-watch": "jest ${1} --watch",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "eslint . --ext .ts --cache",
    "build": "npm run build:ts && npm run build:components",
    "build:components": "componentsjs-generator -s lib",
    "build:ts": "tsc",
    "validate": "npm ls",
    "prepare": "npm run build",
    "version": "manual-git-changelog onversion"
  }
}
